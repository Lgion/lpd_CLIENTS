[32m+[m[32m    `;[m
[32m+[m
[32m+[m[32m    // Envoyer l'email[m
[32m+[m[32m    const info = await transporter.sendMail({[m
[32m+[m[32m      from: {[m
[32m+[m[32m        name: 'Sanctuaire Notre Dame du Rosaire',[m
[32m+[m[32m        address: process.env.EMAIL_USER_MAM[m
[32m+[m[32m      },[m
[32m+[m[32m      to: reservation.email,[m
[32m+[m[32m      bcc: process.env.NEXT_PUBLIC_EMAIL_ADMIN,[m
[32m+[m[32m      subject: 'Confirmation de r√©servation - Sanctuaire Notre Dame du Rosaire',[m
[32m+[m[32m      html: emailContent[m
[32m+[m[32m    });[m
[32m+[m
[32m+[m[32m    console.log('Email envoy√© avec succ√®s:', info.messageId);[m
[32m+[m[32m    return true;[m
[32m+[m[32m  } catch (error) {[m
[32m+[m[32m    console.error('Erreur d√©taill√©e lors de l\'envoi de l\'email:', {[m
[32m+[m[32m      code: error.code,[m
[32m+[m[32m      command: error.command,[m
[32m+[m[32m      response: error.response,[m
[32m+[m[32m      responseCode: error.responseCode,[m
[32m+[m[32m      stack: error.stack[m
[32m+[m[32m    });[m
[32m+[m[41m    [m
[32m+[m[32m    if (error.code === 'ESOCKET') {[m
[32m+[m[32m      console.error('Erreur de connexion au serveur SMTP. V√©rifiez votre connexion Internet et les param√®tres du serveur SMTP.');[m
[32m+[m[32m    } else if (error.code === 'EAUTH') {[m
[32m+[m[32m      console.error('Erreur d\'authentification. V√©rifiez vos identifiants SMTP2GO_USER et SMTP2GO_PASS.');[m
[32m+[m[32m    }[m
[32m+[m[41m    [m
[32m+[m[32m    return false;[m
[32m+[m[32m  }[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32masync function sendPaymentConfirmationEmail(reservation) {[m
[32m+[m[32m  try {[m
[32m+[m[32m    // V√©rifier la connexion SMTP[m
[32m+[m[32m    await transporter.verify();[m
[32m+[m[41m    [m
[32m+[m[32m    // Pr√©parer le contenu de l'email[m
[32m+[m[32m    const emailContent = `[m
[32m+[m[32m      <h2>Confirmation de paiement - Sanctuaire Notre Dame du Rosaire</h2>[m
[32m+[m[32m      <p>Bonjour,</p>[m
[32m+[m[32m      <p>Nous confirmons la r√©ception de votre paiement d'avance de ${reservation.montant_avance} FCFA pour votre r√©servation.</p>[m
[32m+[m[41m      [m
[32m+[m[32m      <h3>Rappel des d√©tails de la r√©servation :</h3>[m
[32m+[m[32m      <ul>[m
[32m+[m[32m        <li>Date d'arriv√©e : ${new Date(reservation.from).toLocaleDateString()}</li>[m
[32m+[m[32m        <li>Date de d√©part : ${new Date(reservation.to).toLocaleDateString()}</li>[m
[32m+[m[32m        <li>Nombre de participants : ${reservation.participants}</li>[m
[32m+[m[32m      </ul>[m
[32m+[m
[32m+[m[32m      <h3>Paiement :</h3>[m
[32m+[m[32m      <ul>[m
[32m+[m[32m        <li>Montant total : ${reservation.montant_total} FCFA</li>[m
[32m+[m[32m        <li>Avance pay√©e : ${reservation.montant_avance} FCFA</li>[m
[32m+[m[32m        <li>Reste √† payer sur place : ${reservation.montant_total - reservation.montant_avance} FCFA</li>[m
[32m+[m[32m      </ul>[m
[32m+[m
[32m+[m[32m      <p>Votre r√©servation est maintenant <strong>confirm√©e</strong>.</p>[m
[32m+[m[41m      [m
[32m+[m[32m      <p>Merci de votre confiance !</p>[m
[32m+[m[41m      [m
[32m+[m[32m      <p style="color: #666; font-size: 0.9em;">[m
[32m+[m[32m        Sanctuaire Notre Dame du Rosaire<br>[m
[32m+[m[32m        Bolobi, Adzop√©<br>[m
[32m+[m[32m        C√¥te d'Ivoire[m
[32m+[m[32m      </p>[m
[32m+[m[32m    `;[m
[32m+[m
[32m+[m[32m    // Envoyer l'email[m
[32m+[m[32m    const info = await transporter.sendMail({[m
[32m+[m[32m      from: {[m
[32m+[m[32m        name: 'Sanctuaire Notre Dame du Rosaire',[m
[32m+[m[32m        address: process.env.SMTP2GO_USER[m
[32m+[m[32m      },[m
[32m+[m[32m      to: reservation.email,[m
[32m+[m[32m      subject: 'Confirmation de paiement - Sanctuaire Notre Dame du Rosaire',[m
[32m+[m[32m      html: emailContent[m
[32m+[m[32m    });[m
[32m+[m
[32m+[m[32m    console.log('Email de confirmation de paiement envoy√© avec succ√®s:', info.messageId);[m
[32m+[m[32m    return true;[m
[32m+[m[32m  } catch (error) {[m
[32m+[m[32m    console.error('Erreur lors de l\'envoi de l\'email de confirmation de paiement:', error);[m
[32m+[m[32m    return false;[m
[32m+[m[32m  }[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32mexport default async function handler(req, res) {[m
[32m+[m[32m  console.log('API - M√©thode re√ßue:', req.method);[m
[32m+[m[32m  console.log('API - Corps de la requ√™te:', req.body);[m
[32m+[m
[32m+[m[32m  // Connexion √† MongoDB si n√©cessaire[m
[32m+[m[32m  if (mongoose.connection.readyState !== 1) {[m
[32m+[m[32m    console.log('API - Connexion √† MongoDB...');[m
[32m+[m[32m    await mongoose.connect(process.env.MONGODB_URI)[m
[32m+[m[32m    console.log('API - Connect√© √† MongoDB');[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  try {[m
[32m+[m[32m    switch (req.method) {[m
[32m+[m[32m      case 'GET':[m
[32m+[m[32m        // R√©cup√©rer toutes les r√©servations[m
[32m+[m[32m        console.log('API - GET - R√©cup√©ration des r√©servations');[m
[32m+[m[32m        const reservations = await modelReservation.find({})[m
[32m+[m[32m          .sort({ from: -1 }) // Trier par date d√©croissante[m
[32m+[m[32m        return res.status(200).json(reservations)[m
[32m+[m
[32m+[m[32m      case 'POST':[m
[32m+[m[32m        // Cr√©er une nouvelle r√©servation[m
[32m+[m[32m        console.log('API - POST - Cr√©ation d\'une nouvelle r√©servation');[m
[32m+[m[32m        console.log('API - POST - Donn√©es re√ßues:', req.body);[m
[32m+[m[41m        [m
[32m+[m[32m        try {[m
[32m+[m[41m          [m
[32m+[m[32m          // const newReservation = await modelReservation.create(req.body.reservation);[m
[32m+[m[32m          // console.log('API - POST - R√©servation cr√©√©e avec succ√®s:', newReservation);[m
[32m+[m[32m          console.log('API - POST - Pas de r√©servation √† cr√©er');[m
[32m+[m[41m          [m
[32m+[m[32m          // Envoyer l'email de confirmation[m
[32m+[m[32m          const tmp = await sendConfirmationEmail({[m
[32m+[m[32m            ...req.body.reservation[m
[32m+[m[32m            // ...newReservation.toObject(),[m
[32m+[m[32m            // email: req.body.reservation.email[m
[32m+[m[32m          });[m
[32m+[m[41m          [m
[32m+[m[32m          return res.status(200).json({blablabla: "msg temporaire"});[m
[32m+[m[41m          [m
[32m+[m[32m          return res.status(201).json(newReservation);[m
[32m+[m[32m        } catch (createError) {[m
[32m+[m[32m          console.error('API - POST - Erreur lors de la cr√©ation:', createError);[m
[32m+[m[32m          return res.status(400).json({[m[41m [m
[32m+[m[32m            message: 'Erreur lors de la cr√©ation de la r√©servation',[m
[32m+[m[32m            error: createError.message,[m
[32m+[m[32m            details: createError[m
[32m+[m[32m          });[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m      case 'PUT':[m
[32m+[m[32m        // Modifier une r√©servation existante[m
[32m+[m[32m        console.log('API - PUT - Modification d\'une r√©servation');[m
[32m+[m[32m        const { id } = req.query;[m
[32m+[m[41m        [m
[32m+[m[32m        // V√©rifier si c'est une validation de paiement[m
[32m+[m[32m        if (req.body.action === 'validatePayment') {[m
[32m+[m[32m          const reservation = await modelReservation.findById(id);[m
[32m+[m[32m          if (!reservation) {[m
[32m+[m[32m            return res.status(404).json({ message: 'R√©servation non trouv√©e' });[m
[32m+[m[32m          }[m
[32m+[m[41m          [m
[32m+[m[32m          // Mettre √† jour le statut de paiement[m
[32m+[m[32m          reservation.avance_payee = true;[m
[32m+[m[32m          reservation.isValidated = true;[m
[32m+[m[41m          [m
[32m+[m[32m          // Envoyer un email de confirmation de paiement[m
[32m+[m[32m          await sendPaymentConfirmationEmail(reservation);[m
[32m+[m[41m          [m
[32m+[m[32m          const updatedReservation = await reservation.save();[m
[32m+[m[32m          return res.status(200).json(updatedReservation);[m
[32m+[m[32m        }[m
[32m+[m[41m        [m
[32m+[m[32m        // Modification normale de la r√©servation[m
[32m+[m[32m        const updatedReservation = await modelReservation.findByIdAndUpdate([m
[32m+[m[32m          id,[m
[32m+[m[32m          req.body,[m
[32m+[m[32m          { new: true }[m
[32m+[m[32m        );[m
[32m+[m[32m        if (!updatedReservation) {[m
[32m+[m[32m          return res.status(404).json({ message: 'R√©servation non trouv√©e' })[m
[32m+[m[32m        }[m
[32m+[m[32m        return res.status(200).json(updatedReservation)[m
[32m+[m
[32m+[m[32m      case 'DELETE':[m
[32m+[m[32m        // Supprimer une r√©servation[m
[32m+[m[32m        console.log('API - DELETE - Suppression d\'une r√©servation');[m
[32m+[m[32m        const deletedReservation = await modelReservation.findByIdAndDelete(req.query.id)[m
[32m+[m[32m        if (!deletedReservation) {[m
[32m+[m[32m          return res.status(404).json({ message: 'R√©servation non trouv√©e' })[m
[32m+[m[32m        }[m
[32m+[m[32m        return res.status(200).json({ message: 'R√©servation supprim√©e avec succ√®s' })[m
[32m+[m
[32m+[m[32m      default:[m
[32m+[m[32m        res.setHeader('Allow', ['GET', 'POST', 'PUT', 'DELETE'])[m
[32m+[m[32m        return res.status(405).json({ message: `Method ${req.method} not allowed` })[m
[32m+[m[32m    }[m
[32m+[m[32m  } catch (error) {[m
[32m+[m[32m    console.error('API - Erreur g√©n√©rale:', error);[m
[32m+[m[32m    return res.status(500).json({[m[41m [m
[32m+[m[32m      message: 'Erreur serveur',[m[41m [m
[32m+[m[32m      error: error.message,[m
[32m+[m[32m      stack: process.env.NODE_ENV === 'development' ? error.stack : undefined[m
[32m+[m[32m    })[m
[32m+[m[32m  }[m
[32m+[m[32m}[m
