[32m+[m[32m"use client"[m
[32m+[m
[32m+[m[32mimport { useState, useEffect } from "react"[m
[32m+[m[32mimport moment from "moment"[m
[32m+[m
[32m+[m[32mmoment.locale('fr')[m
